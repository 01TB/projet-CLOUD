{
  "name": "mon-projet-firebase",
  "version": "1.0.0",
  "description": "Projet Firebase avec Firestore et Functions",
  "scripts": {
    "seed": "node scripts/seed-emulator.js",
    "emulators": "firebase emulators:start",
    "emulators:seed": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "deploy:dev": "firebase use dev && firebase deploy",
    "deploy:prod": "firebase use prod && firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore"
  },
  "devDependencies": {
    "firebase-admin": "^12.0.0"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.13.3",
    "firebase": "^12.8.0"
  }
}
